@charities.map do |charity|
  {
    id:charity.id,
    charity_name:charity.charity_name,
    url:charity.url,
    donation_url: charity.donation_url,
    state:charity.state,
    city:charity.city,
    zip_code:charity.zip_code,
    score:charity.score,
    accepting_donations:charity.accepting_donations,
    category:charity.category,
    deductibility:charity.deductibility,
    mission_statement:charity.mission_statement
}
end

